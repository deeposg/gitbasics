public with sharing class OrderProductTriggerHandler extends TriggerHandler{
    public static List<String> productCodeExceptions = new List<String>{'TTAL', 'JHSCI'};	//product codes that are exceptions to the standard queryies
	public static Set<String> productCodeExceptionsSet = new Set<String>{'TTAL', 'JHSCI'};
    
    public override void beforeInsert() {
        System.debug('OrderProductTriggerHandler::SOQL Use (Start beforeInsert): ' + Limits.getQueries());
        Set<Id> quoteLineIdsSet = new Set<Id>();
        
        for(OrderItem oi : (List<OrderItem>) Trigger.New){
            if(oi.SBQQ__QuoteLine__c != null && (oi.Delivery_Method__c == 'Classroom' || productCodeExceptionsSet.contains(oi.Course_Product_Line__c)) && oi.Classroom_Course__c == NULL) {
                quoteLineIdsSet.add(oi.SBQQ__QuoteLine__c);
            }
        }
        
        if(quoteLineIdsSet.size() > 0){
            OrderProductTriggerHandler.populateClassroom(quoteLineIdsSet);   
        }        

		System.debug('OrderProductTriggerHandler::SOQL Use (End beforeInsert): ' + Limits.getQueries());
    }
    

    public override void afterInsert() {
        System.debug('OrderProductTriggerHandler::SOQL Use (Start afterInsert): ' + Limits.getQueries());
		List<OrderItem> updateOrderItems = new List<OrderItem>();
		List<OrderItem> deliverOrderItems = new List<OrderItem>();
		Set<Id> deliverOrderItemsId = new Set<Id>();
        Set<Id> attendeeOrderItemLink = new Set<Id>(); 
        Map<Id, Id> quoteLineOrderProductMap = new Map<Id, Id>();
        Map<Id, Id> quoteLineServiceAppointmentMap = new Map<Id, Id>();
        
        for(OrderItem oi : (List<OrderItem>) Trigger.New){
			//Deliver any order products that are to be delivered once created
            Product2 prod = [Select Id, Deliver_Immediately__c from Product2 where id = :oi.Product2Id];
            if(prod.Deliver_Immediately__c == TRUE){
                deliverOrderItemsId.add(oi.Id);
            }
            
            if(oi.Course_Product_Line__c != 'AE' && oi.SBQQ__QuoteLine__c != null && (oi.Product_Family__c == 'Courses' || oi.Category__c == 'Onsite' || (oi.Category__c == 'Online' && productCodeExceptionsSet.contains(oi.Course_Product_Line__c)))){
                //attendeeOrderItemLink.add(oi.Id);
                System.debug('AfterInsert::MAP');
                quoteLineOrderProductMap.put(oi.SBQQ__QuoteLine__c, oi.Id);
                quoteLineServiceAppointmentMap.put(oi.SBQQ__QuoteLine__c, oi.Classroom_Course__c);
            }            
        }
        
        /*
        if(attendeeOrderItemLink.size() > 0){
            OrderProductTriggerHandler.linkAttendeeOrderItems(attendeeOrderItemLink);
            OrderProductTriggerHandler.linkAttendeeClassroom(attendeeOrderItemLink);
        }
        */
        
        if(quoteLineOrderProductMap.size() > 0){
            OrderProductTriggerHandler.linkAttendeeOrderItems(quoteLineOrderProductMap);
            OrderProductTriggerHandler.linkAttendeeClassroom(quoteLineServiceAppointmentMap);
        }
        
        if(deliverOrderItemsId.size() > 0){
            for(OrderItem oi : [Select SBQQ__Status__c, Delivery_Date__c from OrderItem where id in: deliverOrderItemsId]){
                deliverOrderItems.add(oi);
            }
            BillingHelper.deliverOrderProducts(deliverOrderItems);
        }     
    }
    
