    //Link Attendee with order product
    @future
    public static void linkAttendeeOrderItems(Set<Id> oiIds){
        List<Attendee__c> updatedAttendees = new List<Attendee__c>();
        for(OrderItem oi : [Select SBQQ__QuoteLine__c from OrderItem where id in: oiIds]){
            for(Attendee__c attendee : [Select Order_Product__c from Attendee__c where Quote_Line__c = :oi.SBQQ__QuoteLine__c]){
                attendee.Order_Product__c = oi.Id;
                updatedAttendees.add(attendee);
            }
        }
        
        if(updatedAttendees.size() > 0){
            update updatedAttendees;
        }
    }

    @future
    public static void linkAttendeeOrderItems(Map<Id, Id> quoteLineOrderProductMap){
        List<Attendee__c> updatedAttendees = new List<Attendee__c>();
        Set<Id> quoteLineIds = new Set<Id>();
        for(Id key : quoteLineOrderProductMap.keySet()){
            quoteLineIds.add(key);
        }

        List<Attendee__c> attendees = [Select Id, Quote_Line__c, Order_Product__c from Attendee__c where Quote_Line__c in :quoteLineIds];
        for(Attendee__c attendee : attendees){
            attendee.Order_Product__c = quoteLineOrderProductMap.get(attendee.Quote_Line__c);
            updatedAttendees.add(attendee);
        }

        if(updatedAttendees.size() > 0){
            update updatedAttendees;
        }
    }
    
    public static void linkAttendeeClassroom(Set<Id> oiIds){
        List<Attendee__c> updatedAttendees = new List<Attendee__c>();
        for(OrderItem oi : [Select SBQQ__QuoteLine__c, Classroom_Course__c from OrderItem where SBQQ__QuoteLine__c != null and Course_Product_Line__c in :productCodeExceptions and id in: oiIds]){
            for(Attendee__c attendee : [Select Service_Appointment__c from Attendee__c where Quote_Line__c = :oi.SBQQ__QuoteLine__c]){
                attendee.Service_Appointment__c = oi.Classroom_Course__c;
                updatedAttendees.add(attendee);
            }
        }
        
        if(updatedAttendees.size() > 0){
            update updatedAttendees;
        }
    }

    public static void linkAttendeeClassroom(Map<Id, Id> quoteLineServiceAppointmentMap){
        List<Attendee__c> updatedAttendees = new List<Attendee__c>();
        set<Id> quoteLineIds = new Set<Id>();
        for(Id key : quoteLineServiceAppointmentMap.keySet()){
            quoteLineIds.add(key);
        }

        List<Attendee__c> attendees = [Select Id, Quote_Line__c, Service_Appointment__c from Attendee__c where Quote_Line__c in :quoteLineIds];
        for(Attendee__c attendee : attendees){
            attendee.Service_Appointment__c = quoteLineServiceAppointmentMap.get(attendee.Quote_Line__c);
            updatedAttendees.add(attendee);
        }

        if(updatedAttendees.size() > 0){
            update updatedAttendees;
        }
    }
    
	public static void populateClassroom(set<Id> qlIds){
        System.debug('OrderProductTriggerHandler::SOQL Use (Start populateClassroom): ' + Limits.getQueries());
        Map<Id,Id> mapQLClassroomIds = new Map<Id, Id>();
        for(SBQQ__QuoteLine__c ql : [Select Id, Classroom_Course__c From SBQQ__QuoteLine__c Where Id IN : qlIds AND Classroom_Course__c!= null]){
            mapQLClassroomIds.put(ql.Id, ql.Classroom_Course__c);
        }
        
        if(mapQLClassroomIds.keyset().size() > 0){
            for(OrderItem item : (List<OrderItem>) Trigger.New){
                if(mapQLClassroomIds.containsKey(item.SBQQ__QuoteLine__c))
                    item.Classroom_Course__c = mapQLClassroomIds.get(item.SBQQ__QuoteLine__c);
            }
        }
        System.debug('OrderProductTriggerHandler::SOQL Use (End populateClassroom): ' + Limits.getQueries());        
    }    
    
    
