public class RecordTypeController {
    public static Map<Id, String> recordtypemap {get;set;}
    
    //Return all record types for Evaluation as strings
    @AuraEnabled        
    public static List<String> getEvaluationRecordTypeValues(){
        List<Schema.RecordTypeInfo> recordtypes = Evaluation__c.SObjectType.getDescribe().getRecordTypeInfos();    
        recordtypemap = new Map<Id, String>();
        for(RecordTypeInfo rt : recordtypes){
            if(rt.isActive()){
	            recordtypemap.put(rt.getRecordTypeId(), rt.getName());                
            }
        }        
        return recordtypemap.values();
    }
    
    //Return ID for Evaluation record type based on string
    @AuraEnabled
    public static Id getEvaluationRecordTypeId(String recordTypeLabel){
        Id recid = Schema.SObjectType.Evaluation__c.getRecordTypeInfosByName().get(recordTypeLabel).getRecordTypeId();        
        return recid;
    }    
    
    //Return ID of contact record for attendee
    @AuraEnabled
    public static Id getContactId(Id attendeeId){
        List<Attendee__c> contactId = [Select Contact__r.Id from Attendee__c where id =: attendeeId limit 1];
        return contactId[0].Contact__r.Id;
    }
    
    //Return true if attendee belongs to a "Evaluations" Product Family SA
    @AuraEnabled
    public static Boolean isSAEvalProduct(Id attendeeId){
        Attendee__c attendee = [Select Service_Appointment__c from Attendee__c where id =: attendeeId];
        ServiceAppointment sa = [Select SA_Product_Family__c from ServiceAppointment where id =: attendee.Service_Appointment__c];
        if(sa.SA_Product_Family__c == 'Evaluations'){
            return true;
        }
        else{
            return false;
        }
    }
    
