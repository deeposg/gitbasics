    @AuraEnabled
    public static List<Attendee__c> getAttLst(Id RecordId){
        System.debug(RecordId);
        List<Attendee__c> attendeeList;
        try{
            attendeeList = [select id, name, Service_Appointment__c, Service_Appointment__r.AppointmentNumber, contact__c,Status__c,Theory_Status__c,Theory_Exam_Results__c from Attendee__c where Service_Appointment__c =: RecordId];
        }catch(Exception e){
            System.debug('Error: '+e.getMessage()+e.getStackTraceString());
        }
         return attendeeList;
    }
    
    @AuraEnabled
    public static String UpdateAttendee(List<Attendee__c> AttendeeList){
        List<Attendee__c> attendee = new List<Attendee__c>();
         try{
            for(Attendee__c attnd : AttendeeList){
            
            attendee.add(new Attendee__c(
                id = attnd.id,contact__c = attnd.contact__c,name = attnd.name ,Status__c = attnd.Status__c,Theory_Status__c = attnd.Theory_Status__c
                ,Theory_Exam_Results__c = attnd.Theory_Exam_Results__c
            ));
        	}
              update attendee;
        }catch(Exception e){
            System.debug('Error: '+e.getMessage()+e.getStackTraceString());
        }
        
       
        return null;
    }
    
