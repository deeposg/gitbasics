    //Wrapper class for Discount Schedules related to Product
    global class Pricing{
        global Decimal min_quantity{get;set;}
        global Decimal max_quantity{get;set;}
        global Decimal price{get;set;}
        
        global Pricing(Decimal min_quantity, Decimal max_quantity, Decimal price){
            this.min_quantity = min_quantity;
            this.max_quantity = max_quantity;
            this.price = price;
        }
    }
    
    //Wrapper class for Service Appointments
    global class Appointments {
        global String id {get;set;}
        global Decimal remaining_quantity {get;set;}
        global DateTime start_date {get;set;}
        global String locations {get;set;}
        global String address{get;set;}
        global String address2{get;set;}
        global String city{get;set;}
        global String state{get;set;}
        global String postal_code{get;set;}
        global DateTime end_date {get;set;}
        global String web_name {get;set;}
        global String province_code {get;set;}
        global Boolean distance_learning {get;set;}
        
        global Appointments(String appointmentId, Decimal quantity, DateTime startDate, DateTime endDate, String locationCity,String address,String address2,String city, String state, String postal_code, String webName, String provinceStateCode, Boolean distanceLearning) {
            id = appointmentId;
            remaining_quantity = quantity;
            start_date = startDate;
            locations = locationCity;
            this.address = address;
            this.address2 = address2;
            this.city = city;
            this.state = state;
            this.postal_code = postal_code;
            end_date = endDate;
            web_name = webName;
            province_code = provinceStateCode;
            distance_learning = distanceLearning;
        }
    }
    
