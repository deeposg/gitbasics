<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>My_Service_Appointment__c</fullName>
    <description>captures that the Assigned resource of Service Appointment is same as current system user</description>
    <externalId>false</externalId>
    <formula>IF($User.Id =  Assigned_Service_Resource__c, true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>My Service Appointment</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
