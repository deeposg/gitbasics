<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Evaluated__c</fullName>
    <description>Captures if the Attendee has been evaluated.</description>
    <externalId>false</externalId>
    <formula>IF(
Count_of_Completed_Evaluations__c &gt; 0,
true,
false
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Indicates if the Attendee has been evaluated.</inlineHelpText>
    <label>Evaluation Passed</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
