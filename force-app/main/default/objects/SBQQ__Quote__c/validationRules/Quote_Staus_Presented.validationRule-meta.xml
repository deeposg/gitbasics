<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quote_Staus_Presented</fullName>
    <active>false</active>
    <errorConditionFormula>and (Text(SBQQ__Status__c) = &apos;Presented&apos;,
not(RecordType.Name = &apos;Approved Quote&apos;)
)</errorConditionFormula>
    <errorMessage>Quote Must Have Record Type Approved To Be Marked As Presented</errorMessage>
</ValidationRule>
