<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_And_Attendee_Must_Match</fullName>
    <active>true</active>
    <errorConditionFormula>not( Attendee__r.Contact__r.Id =  Contact__r.Id )</errorConditionFormula>
    <errorMessage>Attendee Contact Field Must Match Certificate Contact</errorMessage>
</ValidationRule>
