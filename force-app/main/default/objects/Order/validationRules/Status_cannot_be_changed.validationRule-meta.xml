<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_cannot_be_changed</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISCHANGED(Status),
$Profile.Name!= &apos;OSG Sys Admin&apos; 
)</errorConditionFormula>
    <errorMessage>You cannot change the status of an order. Please contact your administrator.</errorMessage>
</ValidationRule>
