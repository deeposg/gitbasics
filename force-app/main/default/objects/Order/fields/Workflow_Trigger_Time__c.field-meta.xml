<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Workflow_Trigger_Time__c</fullName>
    <description>Time based workflow fires an hour after this fields value (NOW - 55 mins + 1 hour = Fire job in 5 mins)</description>
    <externalId>false</externalId>
    <formula>NOW() - 55/1440</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Workflow Trigger Time</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>DateTime</type>
</CustomField>
