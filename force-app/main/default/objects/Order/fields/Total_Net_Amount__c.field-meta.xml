<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Net_Amount__c</fullName>
    <description>Calculates to total Order price as Total -Discounts + Tax</description>
    <externalId>false</externalId>
    <formula>Sub_Total__c +  Shipping_Amount__c +  Total_Tax__c +  Shipping_Tax__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Net Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
