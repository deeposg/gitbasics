<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Review_After_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>and(
not(Validated__c),
 Reviewed__c 
)</errorConditionFormula>
    <errorMessage>Invoice Must Be Validated Before Review</errorMessage>
</ValidationRule>
