<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Force_Primary_Shipping_Address_Type</fullName>
    <active>true</active>
    <errorConditionFormula>and(
 ischanged(Primary_Shipping_Address__c),
Primary_Shipping_Address_Type__c != &apos;Shipping Address&apos;, Primary_Shipping_Address_Type__c != &apos;&apos;)</errorConditionFormula>
    <errorMessage>Primary Shipping Address Must Be A Shipping Address</errorMessage>
</ValidationRule>
