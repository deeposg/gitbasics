<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Force_Primary_Billing_Address_Type</fullName>
    <active>true</active>
    <errorConditionFormula>and(
 ischanged(Primary_Billing_Address__c),
Primary_Billing_Address_Type__c != &apos;Billing Address&apos;, Primary_Billing_Address_Type__c != &apos;&apos;)</errorConditionFormula>
    <errorMessage>Primary Billing Address Must Be A Billing Address</errorMessage>
</ValidationRule>
