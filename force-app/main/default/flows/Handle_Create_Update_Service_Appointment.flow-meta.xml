    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
Text([ServiceAppointment].IH3POS__c) = &apos;On-Site&apos;,
OR(
ISNEW(),
ISCHANGED([ServiceAppointment].SchedStartTime )
)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
Text({!myVariable_current.IH3POS__c}) = &apos;On-Site&apos;,
OR(
ISNEW(),
ISCHANGED({!myVariable_current.SchedStartTime} )
)
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND
(
NOT(ISNEW()),
ISPICKVAL([ServiceAppointment].Status ,&apos;Completed&apos;),
NOT(ISPICKVAL(PRIORVALUE([ServiceAppointment].Status),&apos;Completed&apos;)),
CONTAINS([ServiceAppointment].Product_Name__c, &apos;TTT&apos;),
NOT(CONTAINS([ServiceAppointment].Product_Name__c, &apos;TTTE&apos;))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_10</name>
        <dataType>Boolean</dataType>
        <expression>AND
(
NOT(ISNEW()),
ISPICKVAL({!myVariable_current.Status} ,&apos;Completed&apos;),
NOT(ISPICKVAL(PRIORVALUE({!myVariable_current.Status}),&apos;Completed&apos;)),
CONTAINS({!myVariable_current.Product_Name__c}, &apos;TTT&apos;),
NOT(CONTAINS({!myVariable_current.Product_Name__c}, &apos;TTTE&apos;))
)</expression>
    </formulas>
    <formulas>
