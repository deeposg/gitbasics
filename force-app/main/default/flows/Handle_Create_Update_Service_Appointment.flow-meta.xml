        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND(
{!myVariable_current.Distance_Learning__c} = true,
ISNEW()
)</expression>
    </formulas>
    <formulas>
