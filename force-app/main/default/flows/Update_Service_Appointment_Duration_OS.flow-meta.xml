isblank({!myVariable_current.EarliestStartTime} ),
isblank({!myVariable_current.DueDate}),
Text({!myVariable_current.Status}) = &apos;Completed&apos;,
({!myVariable_current.DueDate} -{!myVariable_current.EarliestStartTime}) &lt;= 0
)))</expression>
    </formulas>
