    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>and(
Text([ServiceAppointment].Status) = &apos;Reschedulable&apos;,
not(or (
Text([ServiceAppointment].Status) = &apos;Canceled&apos;,
isblank([ServiceAppointment].EarliestStartTime ),
isblank([ServiceAppointment].DueDate),
Text([ServiceAppointment].Status) = &apos;Completed&apos;,
([ServiceAppointment].DueDate -[ServiceAppointment].EarliestStartTime) &lt;= 0
)))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>and(
Text({!myVariable_current.Status}) = &apos;Reschedulable&apos;,
not(or (
